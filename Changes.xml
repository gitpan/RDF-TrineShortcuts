    <dc:creator rdf:resource="http://tobyinkster.co.uk/#i"/>
    <dc:description xml:lang="en">Revision history for Perl extension RDF::TrineShortcuts.</dc:description>
    <dc:subject rdf:resource="http://purl.org/NET/cpan-uri/dist/RDF-TrineShortcuts/project"/>
    <dc:title>Changes</dc:title>
  </rdf:Description>
  <Version rdf:about="http://purl.org/NET/cpan-uri/dist/RDF-Trine-Shortcuts/v_0-01">
    <dc:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2010-01-20</dc:issued>
    <file-release rdf:resource="http://backpan.cpan.org/authors/id/T/TO/TOBYINK/RDF-Trine-Shortcuts-0.01.tar.gz"/>
    <revision rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0.01</revision>
    <rdfs:label xml:lang="en">first release</rdfs:label>
  </Version>
  <Version rdf:about="http://purl.org/NET/cpan-uri/dist/RDF-Trine-Shortcuts/v_0-02">
    <dcs:changeset>
      <rdf:Description>
        <dcs:item>
          <dcs:Bugfix>
            <rdfs:label xml:lang="en">Don't break contexts in rdf_parse.</rdfs:label>
          </dcs:Bugfix>
        </dcs:item>
        <dcs:item>
          <dcs:Bugfix>
            <rdfs:label xml:lang="en">rdf_parse should take more than just the first statement from a stream.</rdfs:label>
          </dcs:Bugfix>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Allow rdf_parse to take an RDF::Trine::Model as input.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:versus rdf:resource="http://purl.org/NET/cpan-uri/dist/RDF-Trine-Shortcuts/v_0-01"/>
      </rdf:Description>
    </dcs:changeset>
    <dc:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2010-01-22</dc:issued>
    <file-release rdf:resource="http://backpan.cpan.org/authors/id/T/TO/TOBYINK/RDF-Trine-Shortcuts-0.02.tar.gz"/>
    <revision rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0.02</revision>
  </Version>
  <Version rdf:about="http://purl.org/NET/cpan-uri/dist/RDF-TrineShortcuts/v_0-03">
    <dcs:changeset>
      <rdf:Description>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Module renamed due to popular request. Was 'RDF::Trine::Shortcuts', now 'RDF::TrineShortcuts'.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:versus rdf:resource="http://purl.org/NET/cpan-uri/dist/RDF-Trine-Shortcuts/v_0-02"/>
      </rdf:Description>
    </dcs:changeset>
    <dc:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2010-01-25</dc:issued>
    <file-release rdf:resource="http://backpan.cpan.org/authors/id/T/TO/TOBYINK/RDF-TrineShortcuts-0.03.tar.gz"/>
    <revision rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0.03</revision>
  </Version>
  <Version rdf:about="http://purl.org/NET/cpan-uri/dist/RDF-TrineShortcuts/v_0-04">
    <dcs:changeset>
      <rdf:Description>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">rdf_query() supports non-SPARQL query languages.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">rdf_query() supports relative URIs in queries.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Bundle the 'trapper' command-line tool.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Bundle the 'toquet' command-line tool.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Update>
            <rdfs:label xml:lang="en">use RDF::Trine 0.116</rdfs:label>
          </dcs:Update>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">The rdf_parse() function will use XML::Atom::OWL and RDF::RDFa::Parser to parse Atom+RDFa if these modules are installed.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">When fetching a URI via HTTP(S), rdf_parse() will add data from HTTP Link headers to the resulting model if HTTP::Link::Parser is installed.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">rdf_parse() will use XRD::Parser if it's installed.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:versus rdf:resource="http://purl.org/NET/cpan-uri/dist/RDF-TrineShortcuts/v_0-03"/>
      </rdf:Description>
    </dcs:changeset>
    <dc:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2010-02-04</dc:issued>
    <file-release rdf:resource="http://backpan.cpan.org/authors/id/T/TO/TOBYINK/RDF-TrineShortcuts-0.04.tar.gz"/>
    <revision rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0.04</revision>
  </Version>
  <Version rdf:about="http://purl.org/NET/cpan-uri/dist/RDF-TrineShortcuts/v_0-05">
    <dcs:changeset>
      <rdf:Description>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Fix a packaging error.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:versus rdf:resource="http://purl.org/NET/cpan-uri/dist/RDF-TrineShortcuts/v_0-04"/>
      </rdf:Description>
    </dcs:changeset>
    <dc:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2010-02-04</dc:issued>
    <file-release rdf:resource="http://backpan.cpan.org/authors/id/T/TO/TOBYINK/RDF-TrineShortcuts-0.05.tar.gz"/>
    <revision rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0.05</revision>
  </Version>
  <Version rdf:about="http://purl.org/NET/cpan-uri/dist/RDF-TrineShortcuts/v_0-06">
    <dcs:changeset>
      <rdf:Description>
        <dcs:item>
          <rdf:Description>
            <rdfs:label xml:lang="en">Fix toquet documentation synopsis.</rdfs:label>
          </rdf:Description>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Bundle the 'trist' command-line RDF stats tool.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Bugfix>
            <rdfs:label xml:lang="en">Deal with STDIN correctly in trapper.</rdfs:label>
          </dcs:Bugfix>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">toquet --describe shortcut.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:versus rdf:resource="http://purl.org/NET/cpan-uri/dist/RDF-TrineShortcuts/v_0-05"/>
      </rdf:Description>
    </dcs:changeset>
    <dc:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2010-04-07</dc:issued>
    <file-release rdf:resource="http://backpan.cpan.org/authors/id/T/TO/TOBYINK/RDF-TrineShortcuts-0.06.tar.gz"/>
    <revision rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0.06</revision>
  </Version>
  <Version rdf:about="http://purl.org/NET/cpan-uri/dist/RDF-TrineShortcuts/v_0-07">
    <dcs:changeset>
      <rdf:Description>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Allow rdf_parse (and thus command-line tools) to accept RDF::Trine::Store constructor strings, or RDF::Trine::Store objects.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Bugfix>
            <rdfs:label xml:lang="en">Fix namespaces in trist output.</rdfs:label>
          </dcs:Bugfix>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Allow rdf_string to take a 'namespaces' argument, and provide a default value.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:item>
          <dcs:Addition>
            <rdfs:label xml:lang="en">Provide shortcuts for creating RDF::Trine::Statement and RDF::Trine::Node objects, not exported by default.</rdfs:label>
          </dcs:Addition>
        </dcs:item>
        <dcs:versus rdf:resource="http://purl.org/NET/cpan-uri/dist/RDF-TrineShortcuts/v_0-06"/>
      </rdf:Description>
    </dcs:changeset>
    <dc:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2010-04-11</dc:issued>
    <file-release rdf:resource="http://backpan.cpan.org/authors/id/T/TO/TOBYINK/RDF-TrineShortcuts-0.07.tar.gz"/>
    <revision rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0.07</revision>
  </Version>
